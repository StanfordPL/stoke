@COVERAGE_ENABLED_TRUE@lcov: all
@COVERAGE_ENABLED_TRUE@	$(LCOV) -z -d .
@COVERAGE_ENABLED_TRUE@	(cd src && $(MAKE) $(AM_MAKEFLAGS) check)
